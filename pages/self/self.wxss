page {
  background: #eeeeee;
}

.container {
  width: 100%;
  height: 100%;
}

.header {
  width: 100%;
  height: 340rpx;
  position: relative;
}

.header image {
  position: absolute;
  display: block;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  z-index: 5;
}

